<div class="mx-3">
  <a class="mt-3 w-full btn bg-accent text-white h-12" href="<%= href %>" role="button" style="background-color: #ff0099; height: 48px;">
    <% if (locals.googleIconName != undefined) { %>
      <span class="material-symbols-outlined align-[-6px]" data-bs-toggle="dropdown" aria-expanded="false">
        <%= googleIconName %>
      </span>
    <% } %>
    <%= label %>
  </a>
</div>