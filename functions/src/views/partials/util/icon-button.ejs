<div class="mx-3">
  <button 
    <% if (locals.id != undefined) { %>
      id="<%= id %>" 
    <% } %>
    <% if (locals.typeSubmit != undefined) { %>
      type="submit" 
    <% } %>
    <% if (locals.onClickCode != undefined) { %>
      onclick="<%= onClickCode %>" 
    <% } %>
    class="mt-3 btn text-white w-full bg-accent h-12" style="background-color: #ff0099; height: 48px;">
    <% if (locals.googleIconName != undefined) { %>
      <span class="material-symbols-outlined align-[-6px]">
        <%= googleIconName %>
      </span>
    <% } %>
    <%= label %>
  </button>
</div>