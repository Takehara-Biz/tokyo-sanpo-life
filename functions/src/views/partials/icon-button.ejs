<div class="mx-3">
  <button 
    <% if (locals.buttonId != undefined) { %>
      id="<%= buttonId %>" 
    <% } %>
    <% if (locals.typeSubmit != undefined) { %>
      type="submit" 
    <% } %>
    <% if (locals.onclickCode != undefined) { %>
      onclick="<%= onclickCode %>" 
    <% } %>
    class="mt-3 btn text-white w-full bg-accent h-12" style="background-color: #ff0099; height: 48px;">
    <% if (locals.googleIconName != undefined) { %>
      <span class="material-symbols-outlined align-[-6px]" data-bs-toggle="dropdown" aria-expanded="false">
        <%= googleIconName %>
      </span>
    <% } %>
    <%= label %>
  </button>
</div>